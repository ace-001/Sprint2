package com.cg.hcsAdmin.dto;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class DiagnosticCenter 
{
	@Id
	@Column(name = "center_id")
	private Integer centerId;
	@Column(name = "center_name")
	private String canterName;
	@Column(name = "center_area")
	private String centerArea;
	
	public DiagnosticCenter() {	}

	public DiagnosticCenter(Integer centerId, String canterName, String centerArea) 
	{
		super();
		this.centerId = centerId;
		this.canterName = canterName;
		this.centerArea = centerArea;
	}

	public Integer getCenterId() 
	{
		return centerId;
	}

	public void setCenterId(Integer centerId) 
	{
		this.centerId = centerId;
	}

	public String getCanterName() 
	{
		return canterName;
	}

	public void setCanterName(String canterName) 
	{
		this.canterName = canterName;
	}

	public String getCenterArea() 
	{
		return centerArea;
	}

	public void setCenterArea(String centerArea) 
	{
		this.centerArea = centerArea;
	}
}
